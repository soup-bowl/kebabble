{
    "name": "soup-bowl/kebabble",
    "type": "wordpress-plugin",
    "license": "MIT",
    "homepage": "https://gitlab.com/soup-bowl/kebabble",
    "require": {
        "monolog/monolog": "^1.22",
        "nesbot/carbon": "^2.0.0",
        "twig/twig": "^2.4",
        "soup-bowl/slack-client-php": "0.1.1",
        "soup-bowl/kebabble-order-parser": "0.1",
        "php-di/php-di": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "10up/wp_mock": "^0.4.1"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://gitlab.com/soup-bowl/kebabble-order-parser.git"
        },
        {
            "type": "git",
            "url": "https://gitlab.com/soup-bowl/slack-client-php.git"
        }
    ],
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kebabble\\": "src/"
        }
    }
