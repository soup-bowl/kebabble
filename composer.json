{
    "name": "soup-bowl/kebabble",
    "description" : "Food ordering system for WordPress and Slack.",
    "type": "wordpress-plugin",
    "license": "MIT",
    "homepage": "https://gitlab.com/soup-bowl/kebabble",
    "require": {
        "monolog/monolog": "^2.0",
        "nesbot/carbon": "^2.30",
        "php-di/php-di": "^6.0",
        "botman/botman": "^2.5",
        "botman/driver-slack": "^2.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "10up/wp_mock": "^0.4.1"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kebabble\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KebabbleTests\\": "tests/"
        }
    }
}
