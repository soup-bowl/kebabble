{
    "name": "soup-bowl/kebabble",
    "description" : "Food ordering system for WordPress and Slack.",
    "type": "wordpress-plugin",
    "license": "MIT",
    "homepage": "https://gitlab.com/soup-bowl/kebabble",
    "require": {
        "monolog/monolog": "^1.22",
        "nesbot/carbon": "^2.0.0",
        "php-di/php-di": "^6.0",
        "soup-bowl/slack-client-php": "dev-master",
        "soup-bowl/kebabble-order-parser": "0.1",
        "botman/botman": "^2.5",
        "botman/driver-slack": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "10up/wp_mock": "^0.4.1"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kebabble\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KebabbleTests\\": "tests/"
        }
    }
}
